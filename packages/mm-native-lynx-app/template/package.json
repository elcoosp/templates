{
  "name": "@{{org|kebab_case}}/{{project_name|kebab_case}}",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "pnpm check && rspeedy build && pnpm build:android:kt",
    "build:android:kt": "PROJ_DIR=android/Kotlin{{project_name}} pnpm build-install:android",
    "build:android:java": "PROJ_DIR=android/Java{{project_name}} pnpm build-install:android",
    "build-install:android": "cd $PROJ_DIR && chmod +x ./gradlew && ./gradlew build && ./gradlew installDebug && adb shell monkey -p com.{{org}}.{{project_name|camel_case}} -c android.intent.category.LAUNCHER 1",
    "check": "biome check --write",
    "dev": "rspeedy dev",
    "format": "biome format --write",
    "preview": "rspeedy preview"
  },
  "dependencies": {
    "@lynx-js/react": "^0.105.1",
    "i18next": "24.2.3",
    "react-router": "6.30.0"
  },
  "devDependencies": {
    "unplugin-auto-import": "19.1.1",
    "@biomejs/biome": "^1.9.4",
    "@lynx-js/qrcode-rsbuild-plugin": "^0.3.4",
    "@lynx-js/react-rsbuild-plugin": "^0.9.1",
    "@lynx-js/rspeedy": "^0.8.3",
    "@lynx-contrib/tailwind-preset": "0.0.2",
    "@lynx-js/types": "^3.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "autoprefixer": "10.4.21",
    "rsbuild-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "core-js"]
  },
  "engines": {
    "node": ">=18"
  },
  "private": true
}
