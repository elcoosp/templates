{
  "name": "@{{org|kebab_case}}/{{project_name|kebab_case}}",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rspeedy build && npm run build:android:kt",
    "build:android:kt": "PROJ_DIR=android/Kotlin{{project_name}} npm run build-install:android",
    "build:android:java": "PROJ_DIR=android/Java{{project_name}} npm run build-install:android",
    "build-install:android": "cd $PROJ_DIR && chmod +x ./gradlew && ./gradlew build && adb install app/build/outputs/apk/debug/app-debug.apk && adb shell monkey -p com.{{org}}.{{project_name|camel_case}} -c android.intent.category.LAUNCHER 1",
    "check": "biome check --write",
    "dev": "rspeedy dev",
    "format": "biome format --write",
    "preview": "rspeedy preview"
  },
  "dependencies": {
    "@lynx-js/react": "^0.105.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@lynx-js/qrcode-rsbuild-plugin": "^0.3.4",
    "@lynx-js/react-rsbuild-plugin": "^0.9.1",
    "@lynx-js/rspeedy": "^0.8.3",
    "@lynx-contrib/tailwind-preset":"0.0.2",
    "@lynx-js/types": "^3.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/react": "^19.0.10",
    "autoprefixer":"10.4.21",
    "rsbuild-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "core-js"]
  },
  "engines": {
    "node": ">=18"
  },
  "private": true
}
